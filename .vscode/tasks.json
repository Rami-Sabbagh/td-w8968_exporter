{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "typescript",
			"tsconfig": "tsconfig.json",
			"problemMatcher": [
				"$tsc"
			],
			"group": "build",
			"label": "tsc: build - tsconfig.json",
			"presentation": {
				"echo": true,
				"reveal": "silent",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": false,
				"clear": true,
			}
		},
		{
			"type": "docker-build",
			"label": "docker: build",
			"platform": "node",
			"group": "build",
			"dockerBuild": {
				"context": "${workspaceFolder}",
				"tag": "td-w8968_exporter:latest",
			}
		}
	]
}